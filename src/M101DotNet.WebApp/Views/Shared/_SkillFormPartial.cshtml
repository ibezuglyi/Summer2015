@model System.Collections.Generic.List<WebApp.Models.Skill>

    @for (int i = 0; i < Model.Count; i++)
    {
    <input type="hidden" name="Skills.Index" value="@i" />
    <div class="row">
        <span>
            @Html.TextBoxFor(r => r[i].Name)
        </span>
        <span>
            @Html.TextBoxFor(r => r[i].Level)
        </span>
    </div>
    }
