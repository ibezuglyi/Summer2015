@model WebApp.Models.Offer.OfferModel
@{
    ViewBag.Title = "_OfferTitlePartial";
}

<div class="offer-title">
    <div class="inline-field">
        <div class="card-panel orange lighten-3">
            <span>@Model.Name</span>
        </div>
    </div>
    <div class="inline-field">
        <div class="card-panel orange lighten-3">
            <span>@Model.Salary</span> PLN
        </div>
    </div>
</div> 