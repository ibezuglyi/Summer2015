@model WebApp.Models.Offer.OfferListViewModel

@{
    ViewBag.Title = "Offers list";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@for (int i = Model.OffersList.Count-1; i >= 0 ; i--)
{    
    @Html.Partial("_OfferPartial", Model.OffersList[i])
    @Html.Partial("_RemoveModalPartial", Model.OffersList[i].Offer)
}
@Scripts.Render("~/bundles/offer")

